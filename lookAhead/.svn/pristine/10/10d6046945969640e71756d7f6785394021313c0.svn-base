<div class="container" id="login">
  <form class="form-horizontal" data-toggle="validator" role="form" name="RegForm" action="__ROOT__/index.php?m=Users&a=doRegSave" method="POST">
   
    <div class="form-group">
      <label class="col-xs-2 control-label">用户名：</label>
      <div class="col-xs-10">
        <input type="text" class="form-control" placeholder="用户名" required autofocus>
		<span class="help-block">用户名由字母数字或下划线组成</span>
        <div class="help-block with-errors"></div>
      </div>
    </div>
    <div class="form-group">
      <label class="col-xs-2 control-label">密码：</label>
      <div class="col-xs-10">
        <input type="password" data-minlength="6" class="form-control" id="pwd" name="pwd" placeholder="输入密码" data-match-error="至少6位字幕或数字组成的密码!" required>
        <span class="help-block">至少6位字母或数字组成的密码</span>
        <div class="help-block with-errors"></div>
      </div>
    </div>
    <div class="form-group">
      <label class="col-xs-2 control-label">确认密码：</label>
      <div class="col-xs-10">
        <input class="form-control" type="password" placeholder="再输入一次密码" id="repwd" name="repwd" data-match="#pwd" data-match-error="两次密码输入不一致!" required>
        <div class="help-block with-errors"></div>
      </div>
    </div>
    <div class="form-group">
      <label class="col-xs-2 control-label">姓名:</label>
      <div class="col-xs-10">
        <input class="form-control " type="text" placeholder="姓名" id="contactor" name="contactor" data-error="姓名不能为空" required>
        <div class="help-block with-errors"></div>
      </div>
    </div>
     <div class="form-group">
      <label class="col-xs-2 control-label">年级：</label>
      <div class="col-xs-10">
        <select>
			<option>小学</option>
			<option>中学</option>
			<option>大学</option>
			<option>硕士及硕士以上</option>
		</select>
        <div class="help-block with-errors"></div>
      </div>
    </div>
    <div class="form-group">
      <label class="col-xs-2 control-label">性　　别：</label>
      <div class="col-xs-10">
        <label><input type="radio" name="sex" class="sex" value="男" checked="checked"/>男　　</label><label><input type="radio" name="sex" class="sex" value="女"/>女</label>

        <div class="help-block with-errors"></div>
      </div>
    </div>
    
    
    
    
    <div class="form-group">
      <label class="col-xs-2 control-label">关注科目：</label>
      <div class="col-xs-10">
       <select>
			<option>语文</option>
			<option>数学</option>
			<option>英语</option>
			<option>计算机</option>
			<option>软件工程</option>
			<option>电子商务</option>
			<option>工商管理</option>
		</select>
        <div class="help-block with-errors"></div>
      </div>
    </div>
    <button class="btnLogin"  type="submit">注册</button>
    
  </form>
</div>
