<script src="js/jquery.min.js"></script>
    
    
		<script type="text/javascript">
			function cryptForm()
			{
				if(document.RegForm.pwd.value !="")
				{
					document.RegForm.pwd.value = hex_md5(document.RegForm.pwd.value);
					document.RegForm.repwd.value = hex_md5(document.RegForm.repwd.value);
				}
			}			
